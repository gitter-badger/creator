C['name'] = 'Linux'

C['binsuffix'] = ''
C['bin'] = '$(suffix $0,$binsuffix)'

C['objsuffix'] = 'o'
C['obj'] = '$(suffix $0,$objsuffix)'

C['libsuffix'] = 'a'
C['libprefix'] = 'lib'
C['lib'] = '$(addprefix $libprefix,$(suffix $0,$libsuffix))'

C['dllsuffix'] = 'so'
C['dllprefix'] = 'lib'
C['dll'] = '$(addprefix $dllprefix,$(suffix $0,$dllsuffix))'
